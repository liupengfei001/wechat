<!--index.wxml-->
<view class="container">
  <view class='map-con'>
    <map id="myMap" class="map" longitude="{{longitude}}" latitude="{{latitude}}" scale="14" show-location controls="{{controls}}" bindcontroltap="controlTap" markers="{{markers}}" bindmarkertap="handleMarkerTap"></map>
  </view>
  <view class="bottom">
    <view class="flex-wrp" style="flex-direction:row;">
      <navigator class='flex-item bc_yellow' url="/pages/publish/publish">
      <text>发布</text>
      </navigator>
      <navigator class='flex-item bc_blue' url="/pages/search/search">
      <text>搜索</text>
      </navigator>
    </view>
  </view>
</view>
